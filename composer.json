{
    "name": "crumbls/layup",
    "description": "A visual page builder plugin for Filament 5 â€” Divi-style grid layouts with extensible widgets.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Crumbls\\Layup\\": "src/"
        }
    },
    "autoload-dev": {
